public class Main{
    public static void main(String args[]){
        int i = 0;
        //int i = 2;
        int j = 0;
         try{
             j = 10/i;
         }
         catch(Exception e){
             System.out.println("Something went wrong");
         }
         
         System.out.println(j);
         System.out.println("Bye");
    }
}
