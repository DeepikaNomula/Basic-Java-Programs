public class Main{
    public static void main(String [] args){
        int arr[] = {20, 10, 4, 30, 1};
        int max = arr[0];
        for(int i=0; i<arr.length; i++){
            if (arr[i]>max){
                max = arr[i];
            }
        }
        System.out.println("The Largest number is "+max);
    }
}
