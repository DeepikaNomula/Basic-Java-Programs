class C{
	public void show() {
		System.out.println("in C show");
	}
	static class D{
		public void config() {
			System.out.println("in D config");
		}
	}
}
public class Main{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		C obj = new C();
		obj.show();
		
		C.D obj1 = new  C.D();
		obj1.config();

	}
}
