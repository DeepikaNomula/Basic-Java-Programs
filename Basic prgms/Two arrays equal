import java.util.Arrays;
public class Main{
    public static void main(String args[]){
        int a[] = {1, 2, 34};
        int b[] = {1, 2, 3};
         
         boolean result = Arrays.equals(a,b);
         if(result == true){
             System.out.println("Arrays are equal");
         }
             else{
                 System.out.println("Arrays are not equal");
             }
         }
    }
